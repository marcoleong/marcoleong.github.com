{
  "locals": {
    "url": "http://localhost",
    "name": "Marco's",
    "owner": "Marco Leong",
    "description": "\"The making of life, web application\""
  },
  "port": 8181,
  "output": "../",
  "plugins": [
    "./plugins/paginator.coffee",
    "wintersmith-sass",
    "wintersmith-livereload"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
